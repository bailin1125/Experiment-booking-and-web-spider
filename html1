<script>
$(function(){
	
    var reservePeriodType='2';
    var reserveAdvanceDay='1';
    var reserveStartHH='8';
    var reserveStartMM='0';
    var  week=["星期一 ","星期二","星期三","星期四","星期五","星期六","星期日 "];
    var  sphtml="";
    switch(reservePeriodType){
    case "1":
    	sphtml+="每天的  "+ reserveStartHH+" 时   "+reserveStartMM+" 分  开放接下来  " +reserveAdvanceDay+ "天的预约时间";
    	break;
    case "2":
    	sphtml+="将在每个 "+ ((reserveAdvanceDay>0&&reserveAdvanceDay<8)?week[reserveAdvanceDay-1]:"")+"  的  "+reserveStartHH+" 时  "+reserveStartMM+" 分开放下一周预约";
    	break;
    case "3":
    	sphtml+="将在每月  "+reserveAdvanceDay+" 日   "+reserveStartHH+" 时   "+reserveStartMM+" 分开放下一个月的预约";
    	break;
    }
	$("#sprule").html(sphtml);

	var workDay=['15','15','15','15','15','15','15'];
	for(var i in workDay){
		/* if(d&&(d=parseInt(d))){
			var sprstime="可预约时间：      ";
